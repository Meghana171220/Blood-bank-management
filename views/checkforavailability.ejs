          <h1 style="color: black;margin:2% ;padding: 10px;text-align: center;font-family: 'Bangers', cursive;background-color: #C6EBC9;">Welcome to Blood bank management system</h1>
        <table id="mytable" class="table table-hover table-dark">
            <thead>
                <tr>
                    <th scope="col">User Id</th>
                    <th scope="col">FirstName</th>
                    <th scope="col">Lastname</th>
                    <th scope="col">Email</th>
                    <th scope="col">Phone Number</th>
                    <th scope="col">Address</th>
                    <th scope="col">City</th>
                    <th scope="col">Bloodgroup</th>
                    <th scope="col">view</th>
                    
                </tr>
            </thead>
            <tbody>
                <% print.forEach(function (user) { %>
                  <tr>
                    <td><%= user.u_id %></td>
                      <td><%= user.fname %></td>
                      <td><%= user.lname %></td>
                      <td><%= user.email %></td>
                      <td><%= user.phno %></td>
                      <td><%= user.address %></td>
                      <td><%= user.city %></td>
                      <td><%= user.b_group%></td>
                 
                      <td><a href="/viewinfo/<%=user.u_id%>" class="btn btn-primary" action="/viewinfo/<%=user.u_id%>">viewinfo</a></td>

                      
                  </tr>  
                <% }) %>
            </tbody>
        </table>  
